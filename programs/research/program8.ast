load system io.

function SieveOfEratosthenes(num%int)
  do
    let prime = [true for i in range(num+1)].
    let p = 2.
    while (p*p <= num) do
      if (prime[p] == true) do
        for i in range(p*p,num+1,p) do
          prime[i] = false.
          end
      p = p + 1.
      end

    for p in range(2, num+1) do
      if prime[p] do
        io @println p.
      end
  end
end