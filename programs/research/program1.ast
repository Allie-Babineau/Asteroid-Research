load system io.

function push with (llist:%list, item:%integer) do
  llist@insert(0,item).
end

let llist = [].
push(llist, 20).
push(llist, 4).
push(llist, 15).
push(llist, 85).

io @println "Given Linked List".
io @println llist.

llist@reverse().

io @println "Reversed Linked List".
io @println llist.

  
    
  