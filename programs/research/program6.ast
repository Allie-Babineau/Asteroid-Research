load system io.

function count with (S:%list, m:%integer, n:%integer) do
  let table = [].
  for k in range(0, n+1) do
    table@append(0).
  end
  let table@0 = 1.
  for i in range(0,m) do
    for j in range(S@i,n+1) do
      let table@j = table@j + table@j-(S@i)..
    end
  end
  return table@n.
end

let arr = [1,2,3].
let m = arr@length().
let n = 4.
let x = count(arr,m,n).
io @println x.
--assert x == 4.