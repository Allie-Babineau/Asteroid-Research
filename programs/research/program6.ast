load system io.

function count with (S:%list, m:%integer, n:%integer) do
  let table = [0].
  for k in range(1, n+1) do
    table@append(0).
  end
  for i in range(0,m) do
    for j in range(S[i],n+1) do
      let table[j] = table[j] + table[j-S[i]].
    end
  end
  return table[n].
end

let arr = [1,2,3].
let m = arr@length().
let n = 4.
io @println(count(arr, m, n)).
end