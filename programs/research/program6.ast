load system io.
load system vector.

function count with (S:%vector, m:%integer, n:%integer) do
  let table = [[0 for x in range(m)] for x in range(n+1)].
  for i in range(0,m) do
    table[0][i] = 1.
  end
  for i in range(1,n+1) do
    for j in range(0,m) do
      if i-S[j] >= 0 do
        x = table[i-S[j]][j].
      else do
        x = 0.
      if j >= 1 do
        y = table[i][j-1].
      else do
        y = 0.
      table[x][y] = x+y.
    end
  end
  return table[n][m-1].
end