load system io.
load system math.

function primeFactors with (n%int)

  do  
    while math @mod(n,2) == 0 do
      io @println 2.
      let n = n / 2.
    end

    let i = 3.

    while i < (math @sqrt n)+1 do
      while math @mod(n,i) == 0 do
        io @println i.
        let n = n / i.
      end
      let i = i + 2.
    end

    if n < 2 do
      io @println n.
  end

end