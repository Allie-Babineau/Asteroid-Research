load system io.
load system math.
load system type.

function primeFactors with (n:%integer) do
  while math @mod(n,2) == 0 do
    io@print (2 + " ").
    let n = n / 2.
  end

  for i in 3 to type@tointeger(math@sqrt n)+1 do
    while (math @mod(n,i) == 0) do
      io@print (i + " ").
      let n = n / i.
    end
    let i = i + 1.
  end
  if n > 2 do
    io@println (n).
  end
end

let n = 315.
let res = primeFactors(n).
io @println res.